{
  "name": "trakt-api-explorer",
  "version": "1.0.0",
  "description": "A modern web application for exploring movies using the Trakt API with Node.js backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node scripts/performance_test.js",
    "test-migration": "node scripts/test_migration.js",
    "test-ai": "node scripts/test_ai_integration.js",
    "test-gemini": "node scripts/test_gemini_api.js",
    "test-cache": "node scripts/test_movie_cache.js",
    "quick-start": "node scripts/start_node.js",
    "generate-network": "node scripts/network_generator.js",
    "setup": "npm install && echo 'Copy .env.example to .env and add your API keys'"
